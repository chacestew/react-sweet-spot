{
  "name": "react-sweet-spot",
  "version": "0.1.0",
  "description": "Skeleton for a modern React frontend application and server with great developer utility.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch ./index.js index.js",
    "client": "rimraf dist && NODE_ENV=production webpack --progress --config webpack/client.prod.js",
    "server": "rimraf build/server.js && NODE_ENV=production webpack --progress --config webpack/server.prod.js",
    "build": "npm run client && npm run server",
    "serve": "NODE_ENV=production node index.js",
    "analyze": "NODE_ENV=production webpack --profile --json > stats.json --config webpack/client.prod.js && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "react",
    "webpack",
    "webpack 4",
    "universal",
    "server side rendering",
    "code splitting",
    "babel",
    "babel 7"
  ],
  "author": "Chace Stewart",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "express": "^4.16.4",
    "express-no-favicons": "0.0.1",
    "express-static-gzip": "^1.1.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@loadable/babel-plugin": "^5.2.2",
    "@loadable/webpack-plugin": "^5.2.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "brotli-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "file-loader": "^3.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.7.2"
  }
}
